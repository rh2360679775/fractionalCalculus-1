Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SplitData", "[", 
    RowBox[{"data_", ",", "trainFract_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "data", "]"}]}], ",", "indices", ",", 
       "trainingIndices", ",", "testIndices"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Generate", " ", "random", " ", "indices", " ", "and", " ", "split", 
       " ", "them"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"indices", "=", 
       RowBox[{"RandomSample", "[", 
        RowBox[{"Range", "[", "n", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"trainingIndices", "=", 
       RowBox[{"Take", "[", 
        RowBox[{"indices", ",", 
         RowBox[{"Round", "[", 
          RowBox[{"n", " ", "trainFract"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"testIndices", "=", 
       RowBox[{"Drop", "[", 
        RowBox[{"indices", ",", 
         RowBox[{"Round", "[", 
          RowBox[{"n", " ", "trainFract"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Return", " ", "training", " ", "and", " ", "testing", " ", "data"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"data", "[", 
         RowBox[{"[", "trainingIndices", "]"}], "]"}], ",", 
