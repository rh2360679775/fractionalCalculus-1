Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getCoeffs", "[", "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "xs", ",", "array", ",", "m", ",", "b", ",", "sol", ",", "parms"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xs", "=", 
       RowBox[{"Range", "[", 
        RowBox[{
         RowBox[{"1", "/", "k"}], ",", "1", ",", 
         RowBox[{"1", "/", "k"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"list", " ", "of", " ", "equidistant", " ", "xs"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"array", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"model", "[", "#", "]"}], "&"}], "/@", "xs"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"Coefficient", "[", 
          RowBox[{"#", ",", "as"}], "]"}], "&"}], "/@", "array"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"build", " ", "matrix", " ", "m"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"b", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"f", "[", "#", "]"}], "-", "1"}], "&"}], "/@", "xs"}]}], 
      ";", 
      RowBox[{"(*", 
       RowBox[{"a0", "=", 
        RowBox[{"1", "=", 
         RowBox[{">", 
          RowBox[{"right", " ", "side"}]}]}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"sol", "=", 
       RowBox[{"LinearSolve", "[", 
        RowBox[{"m", ",", "b"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"solve", " ", "the", " ", "system"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"parms", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{"as", "->", "sol"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"list", " ", "of", " ", "parameters"}], "*)"}], 
